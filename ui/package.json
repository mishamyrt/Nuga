{
  "name": "nuga-app-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:dev": "vitest --ui",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format:scripts": "prettier-eslint --config ./.prettierrc --write 'src/**/*.{svelte,ts,scss,css}' 'eslint/*.cjs' '../scripts/**/*.mjs'",
    "format:styles": "stylelint --fix 'src/**/*.{svelte,ts}'",
    "format:misc": "prettier-eslint --config ./.prettierrc --write './*.{md,json,ts,js,cjs}' '../*.{md,json}'",
    "format": "pnpm format:styles && pnpm format:misc && pnpm format:scripts",
    "lint:scripts": "pnpm eslint 'src/**/*.{svelte,ts}'",
    "lint:styles": "pnpm stylelint 'src/**/*.{svelte,css,scss}'",
    "lint": "pnpm run '/^lint:.*/'"
  },
  "devDependencies": {
    "@stylistic/stylelint-config": "^1.0.1",
    "@stylistic/stylelint-plugin": "^2.1.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/deep-equal": "^1.0.4",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitest/coverage-v8": "^1.5.1",
    "@vitest/ui": "^1.5.1",
    "effector": "23.2.1",
    "effector-logger": "^0.15.0",
    "eslint": "^8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-effector": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-svelte": "^2.38.0",
    "feature-sliced-svelte": "^1.4.0",
    "fontkit": "^2.0.2",
    "happy-dom": "^14.7.1",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-scss": "^6.2.1",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.9",
    "svelte-preprocess": "^5.1.4",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vitest": "^1.5.1"
  },
  "dependencies": {
    "@naco-ui/svelte": "1.8.0",
    "colorjs.io": "0.5.0",
    "deep-equal": "2.2.3",
    "effector-queue": "1.0.2",
    "hesoyam": "^0.0.4",
    "patronum": "2.2.0",
    "svelte-awesome-color-picker": "2.4.8",
    "svelte-dnd-action": "^0.9.43"
  }
}
